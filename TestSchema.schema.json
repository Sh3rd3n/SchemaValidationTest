{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "#TestSchema",
  "title": "TestSchema",
  "description": "DOME extensions for TMForum Objects",
  "type": "object",
  "properties": {
    "name": { "type": "string" },
    "brand": { "type": "string" },
    "description": { "type": "string" },
    "productNumber": { "type": "string" },
    "cloudesireProductVersionId": {
      "type": "number",
      "description": "CloudeSire product version identifier"
    },
    "cloudesireResellerCatalogId": {
      "type": "string",
      "description": "CloudeSire reseller catalog identifier"
    }
  },
  "required": ["name", "productNumber"]
}
